<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MultiplePagesNavigationDemo.Page2"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Page2">

    <VerticalStackLayout
        Padding="30,0"
        Spacing="25"
        VerticalOptions="Center">

        <Button
            x:Name="btnPage3"
            Clicked="btnPage3_Clicked"
            HorizontalOptions="Center"
            Text="Go to Page 3" />

        <Button
            x:Name="btnPage1"
            Clicked="btnPage1_Clicked"
            HorizontalOptions="Center"
            Text="Go to Page 1" />

    </VerticalStackLayout>

</ContentPage>